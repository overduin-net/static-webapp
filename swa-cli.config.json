{
  "$schema": "https://aka.ms/azure/static-web-apps-cli/schema",
  "configurations": {
    "cross-fit.wod": {
      "appLocation": "web",
      "apiLocation": "api",
      "outputLocation": "dist/web",
      "appBuildCommand": "npm run build",
      "apiBuildCommand": "dotnet publish -c Release",
      "run": "npm start",
      "appDevserverUrl": "http://localhost:4200"
    }
  }
}